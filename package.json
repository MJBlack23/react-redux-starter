{
  "name": "react-redux-starter",
  "version": "0.0.1",
  "description": "This is boilerplate for a basic react-redux app.  This can be used to scaffold out a very basic app.",
  "main": "index.js",
  "scripts": {
    "start": "browserify --debug --extension=js -o bundle.js src/index.js && http-server -p 8000",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MJBlack23/react-redux-starter.git"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Matt Black",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MJBlack23/react-redux-starter/issues"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "homepage": "https://github.com/MJBlack23/react-redux-starter#readme",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.2.0",
    "http-server": "^0.9.0"
  }
}
